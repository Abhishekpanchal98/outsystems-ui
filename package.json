{
	"name": "outsystems-ui",
	"version": "2.6.9",
	"description": "OutSystems UI Base Theme",
	"scripts": {
		"setup": "npm i && npm i webpack webpack-cli -g",
		"start": "run-script-os",
		"start:darwin:linux": "run-s tab:build:css:mac tab:build:js",
		"start:win32": "wttab -w webpack serve --env prefix=dev tsc --watch",
		"tab:build:css:mac": "ttab webpack serve --env prefix=dev",
		"tab:build:js": "tsc --watch",
		"prod": "eslint **/*.ts && tsc --sourceMap false && webpack --env prefix=prod",
		"lint": "eslint **/*.ts",
		"lintfix": "eslint --fix **/*.ts"
	},
	"author": "OutSystems UI Team",
	"browserslist": [
		"last 2 versions"
	],
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"autoprefixer": "^9.8.6",
		"browserslist": "^4.16.3",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"css-loader": "^5.2.6",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-webpack-plugin": "^3.0.1",
		"html-webpack-plugin": "^5.3.2",
		"marked": "^2.1.1",
		"mini-css-extract-plugin": "^1.6.0",
		"npm-run-all": "^4.1.5",
		"postcss-discard-comments": "^4.0.2",
		"postcss-loader": "^4.2.0",
		"prettier": "^2.3.0",
		"prettier-eslint": "^12.0.0",
		"release-it": "^14.5.0",
		"run-script-os": "^1.1.6",
		"sass": "^1.35.1",
		"sass-loader": "^12.1.0",
		"stylelint": "^13.12.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-order": "^4.1.0",
		"ttab": "^0.7.1",
		"typescript": "^4.3.5",
		"webpack": "^5.47.1",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0",
		"wttab": "^1.2.2"
	}
}
